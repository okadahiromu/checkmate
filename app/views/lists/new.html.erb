<h1>新しいリストを作成</h1>

<%= form_for(@list) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit 'リストを作成' %>
  </div>
<% end %>

<%= link_to '一覧に戻る', lists_path %>